{
  "private": true,
  "name": "umi-study",
  "scripts": {
    "start": "umi dev",
    "analyze": "ANALYZE=1 umi build",
    "HTTPS": "HTTPS=1 umi dev",
    "build": "umi build",
    "webpack": "umi webpack",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.1.1",
    "ahooks": "3.1.3",
    "antd": "4.18.0",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.21",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.1.1",
    "yorkie": "^2.0.0"
  }
}
